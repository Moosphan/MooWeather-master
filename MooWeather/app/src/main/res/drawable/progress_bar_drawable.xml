<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">

        <shape>
            <gradient
                android:angle="0"
                android:centerColor="#94A9C0"
                android:centerY="0.75"
                android:endColor="#B1C2D6"
                android:startColor="#B7C4D2" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">

        <clip>
            <shape>
                <gradient
                    android:angle="0"
                    android:centerColor="#686DE4"
                    android:centerY="0.75"
                    android:endColor="#999CED"
                    android:startColor="#A4A7EA" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">

        <clip>
            <shape>
                <gradient
                    android:angle="0"
                    android:endColor="#68A9F2"
                    android:startColor="#91BEF0" />
            </shape>
        </clip>
    </item>

</layer-list>
